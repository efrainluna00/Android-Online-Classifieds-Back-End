<?xml version="1.0" encoding="UTF-8"?>
<Context path="/eshop/">
<Resource
    name="jdbc/connectorDs"
    auth="Container"
    type="javax.sql.DataSource"
    driverClassName="org.postgresql.Driver"
    url="jdbc:postgresql://localhost:5432/eshop"
    username="postgres"
    password="postgres"
    maxActive="20"
    maxIdle="10"
    maxWait="-1" />
</Context>